<%= form_for(@pocket) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field col-md-9">
    <%= f.text_area :content, placeholder: "アイデアポケット名を入力してください" %>
  </div>
  <div class="col-md-3">
    <%= f.submit "作成", class: "btn btn-primary btn-xs" %>
    <% end %>
  </div>