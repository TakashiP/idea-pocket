<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
<div class="panel-body bar">
    <h2 class = "bar"><span class="label label-info">名前</span>　　　　　 　<%= @user.name %></h2>
    <h2 class = "bar"><span class="label label-info">Email</span>　　　　　　<%= @user.email %></h2>
    <h2 class = "bar"><span class="label label-info">業種</span>             　　　　　　<%= @user.gyokai %></h2>
    <h2 class = "bar"><span class="label label-info">職種</span>　　　　　 　<%= @user.shokushu %></h2>
    <h2 class = "bar"><span class="label label-info">職位</span>　　　　　 　<%= @user.shokui %></h2>
  <br>
</div>